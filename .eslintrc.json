{
  "env": { "browser": true, "node": true },
  "extends": ["eslint:recommended", "plugin:vue/vue3-recommended", "prettier"],
  "rules": {
    "no-console": "off",
    "no-debugger": "off",
    "no-unused-vars": "off",
    "no-unreachable": "off",
    "vue/no-unused-components": "off",
    "vue/multi-word-component-names": "off"
  }
}
